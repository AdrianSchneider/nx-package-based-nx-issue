{
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "dependsOn": [
        "^build"
      ],
      "options": {
        "outputPath": "{workspaceRoot}/dist/{projectName}",
        "main": "{projectRoot}/index.ts",
        "tsConfig": "{projectRoot}/tsconfig.json",
        "_tsConfig": "{workspaceRoot}/tsconfig.json",
        "assets": []
      }
    }
  }
}
